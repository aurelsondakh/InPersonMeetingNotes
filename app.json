{
  "expo": {
    "name": "InPersonMeetingNotesMobileApp",
    "slug": "InPersonMeetingNotesMobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/images/app_logo.png",

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aurelsondakh.inpersonmeetingnotes",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "package": "com.aurelsondakh.inpersonmeetingnotes",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true
    },

    "web": {
      "output": "static"
    },

    "plugins": [
      "./plugins/dist/withBackgroundAudio",
      "expo-router",
      "expo-notifications"
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "07962a15-758c-4e13-a674-e9578b1a86bb"
      }
    },

    "owner": "aurelsondakh"
  }
}
